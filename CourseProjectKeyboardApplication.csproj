<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Images\coding.png" />
    <None Remove="Resources\Images\DevoltonLabsLogo.png" />
    <None Remove="Resources\Images\Keyboard.png" />
    <None Remove="Resources\Images\KeyboardSchemeEng.png" />
    <None Remove="Resources\Images\StartLeftEng.png" />
    <None Remove="Resources\Images\StartRightEng.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MaterialDesignThemes" Version="4.9.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Images\coding.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\DevoltonLabsLogo.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\Keyboard.png" />
    <Resource Include="Resources\Images\KeyboardSchemeEng.png" />
    <Resource Include="Resources\Images\StartLeftEng.png" />
    <Resource Include="Resources\Images\StartRightEng.png" />
  </ItemGroup>
  

</Project>
